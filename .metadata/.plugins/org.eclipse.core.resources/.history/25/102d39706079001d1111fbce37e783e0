from datetime import datetime


def createTransaction(name, nominal, date):
	if date == NILL:
		date = datetime.now()
	print("creating transaction")
	print("name -> " + name)
	print("nominal -> " + nominal)
	print("creating transaction")

createTransaction("belanja", 1000000, NILL)