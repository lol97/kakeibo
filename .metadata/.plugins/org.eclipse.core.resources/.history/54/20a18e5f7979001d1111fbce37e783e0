from datetime import datetime
import pandas as pd


def createTransaction(name, nominal, date = datetime.now()):
	print("creating transaction")
	print("name -> " + name)
	print("nominal -> " + str(nominal))
	print("waktu -> " + str(date))
	print("creating transaction")

createTransaction("belanja", 1000000)